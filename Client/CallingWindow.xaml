<Window x:Class="Client.CallingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="CallingWindow" Height="133.973" Width="300" Loaded="CallingWindowLoaded">
    <Grid RenderTransformOrigin="0.389,0.485">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="23,56,0,0" VerticalAlignment="Top" Width="75" Click="buttonCancel_Click" Grid.Column="1"/>
        <TextBlock x:Name="textBlockInfo" HorizontalAlignment="Left" Margin="47,16,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.ColumnSpan="2" Width="197"/>
        <Button x:Name="buttonAccept" Content="Accept" HorizontalAlignment="Left" Margin="47,56,0,0" VerticalAlignment="Top" Width="75" Click="buttonAccept_Click"/>
        <Button x:Name="buttonDecline" Content="Decline" HorizontalAlignment="Left" Margin="23,56,0,0" VerticalAlignment="Top" Width="75" Grid.Column="1" Click="buttonDecline_Click"/>

    </Grid>
</Window>
